    "@types/cors": "^2.8.17",
    "@types/echarts": "^4.9.22",
    "@types/express": "^4.17.21",
    "@types/faker": "^6.6.8",
    "@types/file-saver": "^2.0.7",
    "@types/js-cookie": "^3.0.6",
    "@types/jsdom": "^21.1.7",
