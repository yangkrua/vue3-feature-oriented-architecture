    "http": "^0.0.1-security",
    "js-cookie": "^3.0.5",
    "jszip": "^3.10.1",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^8.1.0",
    "pinia": "^2.1.7",
    "screenfull": "^6.0.2",
    "sortablejs": "^1.15.3",
    "splitpanes": "^3.1.5",
    "swagger-routes-express": "^3.3.2",
    "vue": "^3.4.29",
    "vue-draggable-next": "^2.2.1",
